{% extends '@SonataPage/base_layout.html.twig' %}

{% block sonata_page_container %}
    
    <header class="navbar flex-column flex-md-row sticky-border-default bg-white">
            {{ sonata_page_render_container('header', page) }}
    </header>
 
    <div class="container" id="container-main">
            <div class="" id="container-sidebar">
                {{ sonata_page_render_container('sidebar', page) }} 
            </div>
        
            <div class="" id="container-content">
                {{ sonata_page_render_container('content', page) }}
      
                {% block page_content %}
                    {% if content is defined %}
                        {{ content|raw }}
                    {% endif %}
                {% endblock %}
            </div>
    
    </div>
        
    <footer class="row">
        {{ sonata_page_render_container('footer', page) }}
    </footer>
    
{% endblock %}
